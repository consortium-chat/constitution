<?xml version="1.0" encoding="utf-8"?>
<schema
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:tome="https://consortium.chat/tome"
  targetNamespace="https://consortium.chat/tome"
  elementFormDefault="qualified">

  <!-- Root element -->
  <element
    name="tome"
    type="tome:tomeType" />

  <!-- Container base -->
  <complexType name="container">
    <sequence>
      <element
        name="title"
        type="tome:titleType" />
      <choice
        minOccurs="0"
        maxOccurs="unbounded">
        <element
          name="text"
          type="tome:textType" />
        <element
          name="list"
          type="tome:listType" />
        <element
          name="reference"
          type="tome:referenceType" />
      </choice>
    </sequence>
    <attribute
      name="id"
      type="tome:idType"
      use="required" />
  </complexType>

  <!-- Tier 1 Container -->
  <complexType name="tomeType">
    <sequence>
      <element
        name="title"
        type="tome:titleType" />
      <element
        name="chapter"
        type="tome:chapterType"
        minOccurs="0"
        maxOccurs="unbounded" />
    </sequence>
    <attribute
      name="id"
      type="tome:idType"
      use="required" />
  </complexType>

  <!-- Tier 2 Container -->
  <complexType name="chapterType">
    <complexContent>
      <extension base="tome:container">
        <sequence>
          <element
            name="section"
            type="tome:sectionType"
            minOccurs="0"
            maxOccurs="unbounded" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <!-- Tier 3+ Container -->
  <complexType name="sectionType">
    <complexContent>
      <extension base="tome:container">
        <sequence>
          <element
            name="section"
            type="tome:sectionType"
            minOccurs="0"
            maxOccurs="unbounded" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <!-- Text without id -->
  <complexType
    name="noidTextType"
    mixed="true">
    <choice
      minOccurs="0"
      maxOccurs="unbounded">
      <element
        name="inlist"
        type="tome:inlistType" />
    </choice>
  </complexType>

  <!-- Text base -->
  <complexType
    name="textType"
    mixed="true">
    <choice
      minOccurs="0"
      maxOccurs="unbounded">
      <element
        name="inlist"
        type="tome:inlistType" />
    </choice>
    <attribute
      name="id"
      type="tome:idType"
      use="required" />
  </complexType>

  <!-- Inline list declaration -->
  <complexType name="inlistType">
    <sequence>
      <element
        name="item"
        type="tome:noidTextType"
        minOccurs="1"
        maxOccurs="unbounded" />
    </sequence>
    <attribute
      name="join"
      type="tome:conjunction"
      use="optional" />
  </complexType>

  <!-- List declaration -->
  <complexType name="listType">
    <sequence>
      <element
        name="intro"
        type="tome:noidTextType"
        minOccurs="0"
        maxOccurs="1" />
      <element
        name="item"
        type="tome:textType"
        minOccurs="1"
        maxOccurs="unbounded" />
    </sequence>
    <attribute
      name="id"
      type="tome:idType"
      use="required" />
    <attribute
      name="join"
      type="tome:conjunction"
      use="optional" />
  </complexType>

  <!-- Container title -->
  <simpleType name="titleType">
    <restriction base="token" />
  </simpleType>

  <!-- External tome reference -->
  <simpleType name="referenceType">
    <list itemType="tome:idType" />
  </simpleType>

  <!-- Conjunction -->
  <simpleType name="conjunction">
    <restriction base="string">
      <enumeration value="and" />
      <enumeration value="or" />
      <enumeration value="andor" />
    </restriction>
  </simpleType>

